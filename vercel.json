{
  "rewrites": [
    { "source": "/old-path", "destination": "/new-path" }
  ],
  "redirects": [
    { "source": "/old-redirect", "destination": "/new-destination", "permanent": true }
  ],
  "headers": [
    { "source": "/(.*)", "headers": [{ "key": "X-Content-Type-Options", "value": "nosniff" }] }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
